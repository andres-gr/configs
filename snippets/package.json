{
  "name": "agr-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "javascript",
          "javascriptreact",
          "jsx",
          "jsx-attr",
          "typescript",
          "typescriptreact"
        ],
        "path": "./snips/typescriptreact.json"
      },
      {
        "language": [
          "css",
          "less",
          "scss"
        ],
        "path": "./snips/css.json"
      },
      {
        "language": "html",
        "path": "./snips/html.json"
      },
      {
        "language": "svelte",
        "path": "./snips/svelte.json"
      }
    ]
  }
}
